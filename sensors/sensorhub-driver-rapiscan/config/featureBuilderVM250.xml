  <fold>
    <name>training.nonemitting</name>
    <foldId>1 10</foldId>
    <recordIds extern="nonemittingTraining.txt"/>
    <requestedRecords ref_id="records"/>
    <classification>
      <actionRelease/>
      <sourceNonEmitting/>
    </classification>
    <laneWidthLimits>3.75 6.25</laneWidthLimits>
  </fold>
  <fold>
