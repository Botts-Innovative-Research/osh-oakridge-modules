<?xml version="1.0" encoding="UTF-8"?>
<featureBuilder xmlns="http://ernie.llnl.gov/tools"
    xmlns:ernie="http://ernie.llnl.gov"
    xmlns:tools="http://ernie.llnl.gov/tools"
    xmlns:vm250="http://ernie.llnl.gov/vm250"
    xmlns:util="http://utility.llnl.gov"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://ernie.llnl.gov http://ernie.llnl.gov/schema/ernie.xsd
                        http://ernie.llnl.gov/vm250 http://ernie.llnl.gov/vm250/schema/ernie-vm250.xsd
                        http://ernie.llnl.gov/tools http://ernie.llnl.gov/tools/schema/ernie-tools.xsd">
  <imports>
    <!-- Pull in the large resources -->
    <vm250:analysis extern="vm250Analysis.xml" id="vm250Analysis"/>
    <vm250:recordDatabase extern="vm250RecordDatabase.xml" id="vm250RecordDatabase"/>
    <vm250:recordManipulator extern="vm250RecordManipulator.xml" id="vm250RecordManipulator"/>
    <vm250:vehicleClassifier extern="vm250VehicleClassifier.xml.gz" id="vehicleClassifier" />
  </imports>
  <defines>
    <util:integer id="records">10000</util:integer>
    <util:integer id="halfrecords">5000</util:integer>
    <util:integer id="SOCrecords">100000</util:integer>
    <!-- Challenge set is 3% the size of "records" -->
    <util:integer id="challengerecords">300</util:integer>

    <util:double id="widthFactor">1.3</util:double>

    <vm250:vehicleSplitAssigner id="vm250VehicleSplitAssigner"/>

    <featureDatabase id="vm250FeatureDatabase">
      <connection>jdbc:h2:test-vm250;DATABASE_TO_UPPER=FALSE</connection>
    </featureDatabase>

    <manipulationGenerator id="source.training.NORM">
      <!-- <sourceList extern="vm250/training-norm-extracted.txt" /> -->
      <sourceList extern="vm250/training-norm.txt" />
      <where>CARGO</where>
      <distributed>1</distributed>
      <label>NORM</label>
      <disableAnisotropy/>
      <conditions>
        <minimumGrossSNR limit="4"/>
      </conditions>
      <intensity>50 500</intensity>
    </manipulationGenerator>

    <manipulationGenerator id="source.training.Fissile">
      <cargoDistributions> 2 4 2 1 1 </cargoDistributions>
      <sourceList extern="vm250/training-fissile.txt" />
      <where>CARGO</where>
      <label>Fissile</label>
      <anywhereProbability>0.2</anywhereProbability>
      <conditions>
        <minimumInjectedSNR limit="8"/>
      </conditions>
      <intensity>2000 80000</intensity>
    </manipulationGenerator>

    <manipulationGenerator id="source.test.Fissile">
      <cargoDistributions> 2 4 2 1 1 </cargoDistributions>
      <sourceList extern="vm250/testing-fissile.txt" />
      <where>CARGO</where>
      <label>Fissile</label>
      <anywhereProbability>0.2</anywhereProbability>
      <conditions>
        <minimumInjectedSNR limit="8"/>
        <maximumInjectedSNR limit="80"/>
      </conditions>
      <intensity>1 1</intensity>
    </manipulationGenerator>

    <manipulationGenerator id="source.training.Medical">
      <cargoDistributions> 2 4 2 1 1 </cargoDistributions>
      <sourceList extern="vm250/training-medical.txt" />
      <where>DRIVER</where>
      <label>Medical</label>
      <passengerSideProbability>0.5</passengerSideProbability>
      <anywhereProbability>0.05</anywhereProbability>
      <conditions>
        <minimumInjectedSNR limit="8"/>
      </conditions>
      <intensity>450 30000</intensity>
    </manipulationGenerator>

    <manipulationGenerator id="source.test.Medical">
      <cargoDistributions> 2 4 2 1 1 </cargoDistributions>
      <sourceList extern="vm250/testing-medical.txt" />
      <where>DRIVER</where>
      <label>Medical</label>
      <passengerSideProbability>0.5</passengerSideProbability>
      <anywhereProbability>0.05</anywhereProbability>
      <conditions>
        <minimumInjectedSNR limit="8"/>
        <maximumInjectedSNR limit="80"/>
      </conditions>
      <intensity>1 1</intensity>
    </manipulationGenerator>

    <manipulationGenerator id="source.training.Industrial">
      <cargoDistributions> 2 4 2 1 1 </cargoDistributions>
      <sourceList extern="vm250/training-industrial.txt" />
      <where>CARGO</where>
      <label>Industrial</label>
      <anywhereProbability>0.2</anywhereProbability>
      <conditions>
        <minimumInjectedSNR limit="8"/>
      </conditions>
      <intensity>500 30000</intensity>
    </manipulationGenerator>

    <manipulationGenerator id="source.test.Industrial">
      <cargoDistributions> 2 4 2 1 1 </cargoDistributions>
      <sourceList extern="vm250/testing-industrial.txt" />
      <where>CARGO</where>
      <label>Industrial</label>
      <anywhereProbability>0.2</anywhereProbability>
      <conditions>
        <minimumInjectedSNR limit="10"/>
        <maximumInjectedSNR limit="100"/>
      </conditions>
      <intensity>1 1</intensity>
    </manipulationGenerator>

    <manipulationGenerator id="source.training.Contamination">
      <cargoDistributions> 2 4 2 1 1 </cargoDistributions>
      <sourceList extern="vm250/training-contamination.txt" />
      <where>CARGO</where>
      <label>Industrial</label>
      <maxDistributedLength>1.2</maxDistributedLength>
      <anywhereProbability>0.2</anywhereProbability>
      <distributedProbability>0.1</distributedProbability>
      <conditions>
        <minimumInjectedSNR limit="8"/>
      </conditions>
      <intensity>200 10000</intensity>
    </manipulationGenerator>

    <manipulationGenerator id="source.training.CNM">
      <cargoDistributions> 2 4 2 1 1 </cargoDistributions>
      <sourceList extern="vm250/training-CNM.txt" />
      <where>CARGO</where>
      <label>Industrial</label>
      <anywhereProbability>0.2</anywhereProbability>
      <conditions>
        <minimumInjectedSNR limit="8"/>
      </conditions>
      <intensity>2000 300000</intensity>
    </manipulationGenerator>

    <manipulationGenerator id="source.training.CNM.distributed">
      <cargoDistributions> 2 4 2 1 1 </cargoDistributions>
      <sourceList extern="vm250/training-CNM.txt" />
      <where>CARGO</where>
      <distributed>1</distributed>
      <label>Industrial</label>
      <conditions>
        <minimumInjectedSNR limit="50"/>
      </conditions>
      <intensity>1000 200000</intensity>
    </manipulationGenerator>


    <!-- The next 3 source types are the challenge sets. Maximum intensity is 10x the training set -->
    <manipulationGenerator id="source.cargo.test.challenge.Medical">
      <cargoDistributions> 2 4 2 1 1 </cargoDistributions>
      <sourceList extern="vm250/training-medical.txt" />
      <where>DRIVER</where>
      <label>Medical</label>
      <anywhereProbability>0.1</anywhereProbability> 
      <passengerSideProbability>0.5</passengerSideProbability>
      <conditions>
        <minimumInjectedSNR limit="10"/>
      </conditions>
      <intensity>450 300000</intensity>
    </manipulationGenerator>
    
    <manipulationGenerator id="source.cargo.test.challenge.Industrial">
      <cargoDistributions> 2 4 2 1 1 </cargoDistributions>
      <sourceList extern="vm250/training-industrial.txt" />
      <where>CARGO</where>
      <label>Industrial</label>
      <maxDistributedLength>1.2</maxDistributedLength>
      <anywhereProbability>0.2</anywhereProbability>
      <distributedProbability>0.1</distributedProbability>
      <conditions>
        <minimumInjectedSNR limit="10"/>
      </conditions>
      <intensity>500 300000</intensity>
    </manipulationGenerator>
    
    <manipulationGenerator id="source.cargo.test.challenge.Fissile">
      <cargoDistributions> 2 4 2 1 1 </cargoDistributions>
      <sourceList extern="vm250/training-fissile.txt" />
      <where>CARGO</where>
      <label>Fissile</label>
      <maxDistributedLength>1.2</maxDistributedLength>
      <anywhereProbability>0.2</anywhereProbability>
      <distributedProbability>0.1</distributedProbability>
      <conditions>
        <minimumInjectedSNR limit="10"/>
      </conditions>
      <intensity>2000 800000</intensity>
    </manipulationGenerator>

  </defines>
  <analysis ref_id="vm250Analysis"/>

  <preprocessors>
    <vm250:qualityCheck/>
    <vm250:gammaPanelFix ttbr="1"/>
    <vm250:randomMotionProfiler>
      <vm250:model extern="seaport.bin"/>
    </vm250:randomMotionProfiler>
    <vm250:vehicleClassifier ref_id="vehicleClassifier"/>
    <vm250:backgroundEstimator/>
  </preprocessors>

  <featureDatabase ref_id="vm250FeatureDatabase"/>
  <vehicleSplitAssigner ref_id="vm250VehicleSplitAssigner"/>
  <recordDatabase ref_id="vm250RecordDatabase"/>
  <recordManipulator ref_id="vm250RecordManipulator"/>

